import type { BreakpointsT } from '../styles';

/**
 * Helper function that generates media queries based on breakpoint, e.g.
 * getMediaQuery(720) => '@media screen and (min-width: 720px)'
 */
declare export var getMediaQuery: (breakpoint: number) => string;

declare export var getMediaQueries: (breakpoints: BreakpointsT) => string[];
